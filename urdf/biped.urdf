<?xml version="1.0"?>
<robot name="biped_3r_floating_with_upper_body">

  <!-- =======================
       Root: pelvis (FREE-FLYER)
       ======================= -->
  <link name="pelvis_link">
    <inertial>
      <origin xyz="0 0 0.10" rpy="0 0 0"/>
      <mass value="25.0"/>
      <inertia ixx="0.20" ixy="0.0" ixz="0.0"
               iyy="0.35" iyz="0.0"
               izz="0.20"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.10" rpy="0 0 0"/>
      <geometry><box size="0.20 0.12 0.30"/></geometry>
      <material name="pelvis_red"><color rgba="0.9 0.2 0.2 1.0"/></material>
    </visual>
    <collision>
      <origin xyz="0 0 0.10" rpy="0 0 0"/>
      <geometry><box size="0.20 0.12 0.30"/></geometry>
    </collision>
  </link>

  <!-- =======================
       Upper body (simple)
       ======================= -->
  <link name="torso_link">
    <inertial>
      <!-- torso frame at its base; COM somewhere mid-height -->
      <origin xyz="0 0 0.18" rpy="0 0 0"/>
      <mass value="15.0"/>
      <inertia ixx="0.18" ixy="0" ixz="0"
               iyy="0.22" iyz="0"
               izz="0.18"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.18" rpy="0 0 0"/>
      <geometry><box size="0.26 0.16 0.36"/></geometry>
      <material name="torso_purple"><color rgba="0.55 0.35 0.9 1.0"/></material>
    </visual>
    <collision>
      <origin xyz="0 0 0.18" rpy="0 0 0"/>
      <geometry><box size="0.26 0.16 0.36"/></geometry>
    </collision>
  </link>

  <!-- Attach torso on top of pelvis box (pelvis top is at z=0.25) -->
  <joint name="torso_fixed" type="fixed">
    <parent link="pelvis_link"/>
    <child link="torso_link"/>
    <origin xyz="0 0 0.25" rpy="0 0 0"/>
  </joint>

  <link name="head_link">
    <inertial>
      <origin xyz="0 0 0.07" rpy="0 0 0"/>
      <mass value="4.0"/>
      <inertia ixx="0.01" ixy="0" ixz="0"
               iyy="0.01" iyz="0"
               izz="0.01"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0.07" rpy="0 0 0"/>
      <geometry><sphere radius="0.08"/></geometry>
      <material name="head_white"><color rgba="0.95 0.95 0.95 1.0"/></material>
    </visual>
    <collision>
      <origin xyz="0 0 0.07" rpy="0 0 0"/>
      <geometry><sphere radius="0.08"/></geometry>
    </collision>
  </link>

  <!-- Head at top of torso (torso height 0.36, so top around z=0.36 in torso link frame) -->
  <joint name="head_fixed" type="fixed">
    <parent link="torso_link"/>
    <child link="head_link"/>
    <origin xyz="0 0 0.36" rpy="0 0 0"/>
  </joint>

  <link name="left_arm_link">
    <inertial>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <mass value="2.0"/>
      <inertia ixx="0.02" ixy="0" ixz="0"
               iyy="0.02" iyz="0"
               izz="0.002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <geometry><cylinder radius="0.03" length="0.30"/></geometry>
      <material name="arm_cyan_L"><color rgba="0.2 0.85 0.85 1.0"/></material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <geometry><cylinder radius="0.03" length="0.30"/></geometry>
    </collision>
  </link>

  <joint name="left_arm_fixed" type="fixed">
    <parent link="torso_link"/>
    <child link="left_arm_link"/>
    <!-- shoulder -->
    <origin xyz="0.00 0.14 0.28" rpy="0 0 0"/>
  </joint>

  <link name="right_arm_link">
    <inertial>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <mass value="2.0"/>
      <inertia ixx="0.02" ixy="0" ixz="0"
               iyy="0.02" iyz="0"
               izz="0.002"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <geometry><cylinder radius="0.03" length="0.30"/></geometry>
      <material name="arm_cyan_R"><color rgba="0.2 0.75 0.9 1.0"/></material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.15" rpy="0 0 0"/>
      <geometry><cylinder radius="0.03" length="0.30"/></geometry>
    </collision>
  </link>

  <joint name="right_arm_fixed" type="fixed">
    <parent link="torso_link"/>
    <child link="right_arm_link"/>
    <origin xyz="0.00 -0.14 0.28" rpy="0 0 0"/>
  </joint>

  <!-- =========================================================
       LEGS: identical to biped_3r_floating (copied verbatim)
       ========================================================= -->

  <!-- LEFT LEG -->
  <link name="thigh_link_L">
    <inertial>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <mass value="7.0"/>
      <inertia ixx="0.12" ixy="0.0" ixz="0.0"
               iyy="0.12" iyz="0.0"
               izz="0.01"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry><cylinder radius="0.040" length="0.45"/></geometry>
      <material name="thigh_green_L"><color rgba="0.2 0.8 0.3 1.0"/></material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry><cylinder radius="0.040" length="0.45"/></geometry>
    </collision>
  </link>

  <joint name="hip_pitch_L" type="revolute">
    <origin xyz="0 0.08 0.0" rpy="0 0 0"/>
    <parent link="pelvis_link"/>
    <child link="thigh_link_L"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.5" upper="1.5" effort="250" velocity="10.0"/>
    <dynamics damping="0.3" friction="0.0"/>
  </joint>

  <link name="shank_link_L">
    <inertial>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <mass value="5.0"/>
      <inertia ixx="0.085" ixy="0.0" ixz="0.0"
               iyy="0.085" iyz="0.0"
               izz="0.008"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry><cylinder radius="0.035" length="0.45"/></geometry>
      <material name="shank_blue_L"><color rgba="0.2 0.3 0.9 1.0"/></material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry><cylinder radius="0.035" length="0.45"/></geometry>
    </collision>
  </link>

  <joint name="knee_pitch_L" type="revolute">
    <origin xyz="0 0 -0.45" rpy="0 0 0"/>
    <parent link="thigh_link_L"/>
    <child link="shank_link_L"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.5" upper="0.1" effort="250" velocity="10.0"/>
    <dynamics damping="0.3" friction="0.0"/>
  </joint>

  <link name="foot_link_L">
    <inertial>
      <origin xyz="0 0 -0.015" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.0020" ixy="0.0" ixz="0.0"
               iyy="0.0040" iyz="0.0"
               izz="0.0020"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.015" rpy="0 0 0"/>
      <geometry><box size="0.24 0.08 0.03"/></geometry>
      <material name="foot_gray_L"><color rgba="0.5 0.5 0.5 1.0"/></material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.015" rpy="0 0 0"/>
      <geometry><box size="0.24 0.08 0.03"/></geometry>
    </collision>
  </link>

  <joint name="ankle_pitch_L" type="revolute">
    <origin xyz="0 0 -0.45" rpy="0 0 0"/>
    <parent link="shank_link_L"/>
    <child link="foot_link_L"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.2" upper="0.8" effort="150" velocity="8.0"/>
    <dynamics damping="0.2" friction="0.0"/>
  </joint>

  <link name="heel_contact_L">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1e-6"/>
      <inertia ixx="1e-9" ixy="0" ixz="0"
               iyy="1e-9" iyz="0"
               izz="1e-9"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry><sphere radius="0.01"/></geometry>
      <material name="heel_yellow_L"><color rgba="0.9 0.9 0.2 1.0"/></material>
    </visual>
  </link>

  <joint name="heel_contact_fixed_L" type="fixed">
    <parent link="foot_link_L"/>
    <child link="heel_contact_L"/>
    <origin xyz="-0.12 0 -0.03" rpy="0 0 0"/>
  </joint>

  <link name="toe_contact_L">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1e-6"/>
      <inertia ixx="1e-9" ixy="0" ixz="0"
               iyy="1e-9" iyz="0"
               izz="1e-9"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry><sphere radius="0.01"/></geometry>
      <material name="toe_orange_L"><color rgba="0.9 0.5 0.2 1.0"/></material>
    </visual>
  </link>

  <joint name="toe_contact_fixed_L" type="fixed">
    <parent link="foot_link_L"/>
    <child link="toe_contact_L"/>
    <origin xyz="0.12 0 -0.03" rpy="0 0 0"/>
  </joint>

  <!-- RIGHT LEG -->
  <link name="thigh_link_R">
    <inertial>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <mass value="7.0"/>
      <inertia ixx="0.12" ixy="0.0" ixz="0.0"
               iyy="0.12" iyz="0.0"
               izz="0.01"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry><cylinder radius="0.040" length="0.45"/></geometry>
      <material name="thigh_green_R"><color rgba="0.2 0.75 0.35 1.0"/></material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry><cylinder radius="0.040" length="0.45"/></geometry>
    </collision>
  </link>

  <joint name="hip_pitch_R" type="revolute">
    <origin xyz="0 -0.08 0.0" rpy="0 0 0"/>
    <parent link="pelvis_link"/>
    <child link="thigh_link_R"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.5" upper="1.5" effort="250" velocity="10.0"/>
    <dynamics damping="0.3" friction="0.0"/>
  </joint>

  <link name="shank_link_R">
    <inertial>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <mass value="5.0"/>
      <inertia ixx="0.085" ixy="0.0" ixz="0.0"
               iyy="0.085" iyz="0.0"
               izz="0.008"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry><cylinder radius="0.035" length="0.45"/></geometry>
      <material name="shank_blue_R"><color rgba="0.25 0.35 0.95 1.0"/></material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry><cylinder radius="0.035" length="0.45"/></geometry>
    </collision>
  </link>

  <joint name="knee_pitch_R" type="revolute">
    <origin xyz="0 0 -0.45" rpy="0 0 0"/>
    <parent link="thigh_link_R"/>
    <child link="shank_link_R"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.5" upper="0.1" effort="250" velocity="10.0"/>
    <dynamics damping="0.3" friction="0.0"/>
  </joint>

  <link name="foot_link_R">
    <inertial>
      <origin xyz="0 0 -0.015" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.0020" ixy="0.0" ixz="0.0"
               iyy="0.0040" iyz="0.0"
               izz="0.0020"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.015" rpy="0 0 0"/>
      <geometry><box size="0.24 0.08 0.03"/></geometry>
      <material name="foot_gray_R"><color rgba="0.52 0.52 0.52 1.0"/></material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.015" rpy="0 0 0"/>
      <geometry><box size="0.24 0.08 0.03"/></geometry>
    </collision>
  </link>

  <joint name="ankle_pitch_R" type="revolute">
    <origin xyz="0 0 -0.45" rpy="0 0 0"/>
    <parent link="shank_link_R"/>
    <child link="foot_link_R"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.2" upper="0.8" effort="150" velocity="8.0"/>
    <dynamics damping="0.2" friction="0.0"/>
  </joint>

  <link name="heel_contact_R">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1e-6"/>
      <inertia ixx="1e-9" ixy="0" ixz="0"
               iyy="1e-9" iyz="0"
               izz="1e-9"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry><sphere radius="0.01"/></geometry>
      <material name="heel_yellow_R"><color rgba="0.95 0.9 0.25 1.0"/></material>
    </visual>
  </link>

  <joint name="heel_contact_fixed_R" type="fixed">
    <parent link="foot_link_R"/>
    <child link="heel_contact_R"/>
    <origin xyz="-0.12 0 -0.03" rpy="0 0 0"/>
  </joint>

  <link name="toe_contact_R">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1e-6"/>
      <inertia ixx="1e-9" ixy="0" ixz="0"
               iyy="1e-9" iyz="0"
               izz="1e-9"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry><sphere radius="0.01"/></geometry>
      <material name="toe_orange_R"><color rgba="0.95 0.55 0.25 1.0"/></material>
    </visual>
  </link>

  <joint name="toe_contact_fixed_R" type="fixed">
    <parent link="foot_link_R"/>
    <child link="toe_contact_R"/>
    <origin xyz="0.12 0 -0.03" rpy="0 0 0"/>
  </joint>

</robot>
