<?xml version="1.0"?>
<robot name="biped_3r_floating">

  <!-- =======================
       Root link: pelvis (FREE-FLYER in Pinocchio)
       x forward, z up, y out of sagittal plane
       ======================= -->
  <link name="pelvis_link">
    <inertial>
      <origin xyz="0 0 0.10" rpy="0 0 0"/>
      <mass value="25.0"/>
      <inertia ixx="0.20" ixy="0.0" ixz="0.0"
               iyy="0.35" iyz="0.0"
               izz="0.20"/>
    </inertial>

    <visual>
      <origin xyz="0 0 0.10" rpy="0 0 0"/>
      <geometry>
        <box size="0.20 0.12 0.30"/>
      </geometry>
      <material name="pelvis_red">
        <color rgba="0.9 0.2 0.2 1.0"/>
      </material>
    </visual>

    <collision>
      <origin xyz="0 0 0.10" rpy="0 0 0"/>
      <geometry>
        <box size="0.20 0.12 0.30"/>
      </geometry>
    </collision>
  </link>

  <!-- =========================================================
       LEFT LEG (suffix _L), hip offset +y
       ========================================================= -->
  <link name="thigh_link_L">
    <inertial>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <mass value="7.0"/>
      <inertia ixx="0.12" ixy="0.0" ixz="0.0"
               iyy="0.12" iyz="0.0"
               izz="0.01"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.040" length="0.45"/>
      </geometry>
      <material name="thigh_green_L">
        <color rgba="0.2 0.8 0.3 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.040" length="0.45"/>
      </geometry>
    </collision>
  </link>

  <joint name="hip_pitch_L" type="revolute">
    <!-- hip lateral offset -->
    <origin xyz="0 0.08 0.0" rpy="0 0 0"/>
    <parent link="pelvis_link"/>
    <child link="thigh_link_L"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.5" upper="1.5" effort="250" velocity="10.0"/>
    <dynamics damping="0.3" friction="0.0"/>
  </joint>

  <link name="shank_link_L">
    <inertial>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <mass value="5.0"/>
      <inertia ixx="0.085" ixy="0.0" ixz="0.0"
               iyy="0.085" iyz="0.0"
               izz="0.008"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.035" length="0.45"/>
      </geometry>
      <material name="shank_blue_L">
        <color rgba="0.2 0.3 0.9 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.035" length="0.45"/>
      </geometry>
    </collision>
  </link>

  <joint name="knee_pitch_L" type="revolute">
    <origin xyz="0 0 -0.45" rpy="0 0 0"/>
    <parent link="thigh_link_L"/>
    <child link="shank_link_L"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.5" upper="0.1" effort="250" velocity="10.0"/>
    <dynamics damping="0.3" friction="0.0"/>
  </joint>

  <link name="foot_link_L">
    <inertial>
      <origin xyz="0 0 -0.015" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.0020" ixy="0.0" ixz="0.0"
               iyy="0.0040" iyz="0.0"
               izz="0.0020"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.015" rpy="0 0 0"/>
      <geometry>
        <box size="0.24 0.08 0.03"/>
      </geometry>
      <material name="foot_gray_L">
        <color rgba="0.5 0.5 0.5 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.015" rpy="0 0 0"/>
      <geometry>
        <box size="0.24 0.08 0.03"/>
      </geometry>
    </collision>
  </link>

  <joint name="ankle_pitch_L" type="revolute">
    <origin xyz="0 0 -0.45" rpy="0 0 0"/>
    <parent link="shank_link_L"/>
    <child link="foot_link_L"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.2" upper="0.8" effort="150" velocity="8.0"/>
    <dynamics damping="0.2" friction="0.0"/>
  </joint>

  <!-- Heel / Toe contact links (LEFT) -->
  <link name="heel_contact_L">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1e-6"/>
      <inertia ixx="1e-9" ixy="0" ixz="0"
               iyy="1e-9" iyz="0"
               izz="1e-9"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry><sphere radius="0.01"/></geometry>
      <material name="heel_yellow_L"><color rgba="0.9 0.9 0.2 1.0"/></material>
    </visual>
  </link>

  <joint name="heel_contact_fixed_L" type="fixed">
    <parent link="foot_link_L"/>
    <child link="heel_contact_L"/>
    <origin xyz="-0.12 0 -0.03" rpy="0 0 0"/>
  </joint>

  <link name="toe_contact_L">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1e-6"/>
      <inertia ixx="1e-9" ixy="0" ixz="0"
               iyy="1e-9" iyz="0"
               izz="1e-9"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry><sphere radius="0.01"/></geometry>
      <material name="toe_orange_L"><color rgba="0.9 0.5 0.2 1.0"/></material>
    </visual>
  </link>

  <joint name="toe_contact_fixed_L" type="fixed">
    <parent link="foot_link_L"/>
    <child link="toe_contact_L"/>
    <origin xyz="0.12 0 -0.03" rpy="0 0 0"/>
  </joint>

  <!-- =========================================================
       RIGHT LEG (suffix _R), hip offset -y
       ========================================================= -->
  <link name="thigh_link_R">
    <inertial>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <mass value="7.0"/>
      <inertia ixx="0.12" ixy="0.0" ixz="0.0"
               iyy="0.12" iyz="0.0"
               izz="0.01"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.040" length="0.45"/>
      </geometry>
      <material name="thigh_green_R">
        <color rgba="0.2 0.75 0.35 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.040" length="0.45"/>
      </geometry>
    </collision>
  </link>

  <joint name="hip_pitch_R" type="revolute">
    <origin xyz="0 -0.08 0.0" rpy="0 0 0"/>
    <parent link="pelvis_link"/>
    <child link="thigh_link_R"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.5" upper="1.5" effort="250" velocity="10.0"/>
    <dynamics damping="0.3" friction="0.0"/>
  </joint>

  <link name="shank_link_R">
    <inertial>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <mass value="5.0"/>
      <inertia ixx="0.085" ixy="0.0" ixz="0.0"
               iyy="0.085" iyz="0.0"
               izz="0.008"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.035" length="0.45"/>
      </geometry>
      <material name="shank_blue_R">
        <color rgba="0.25 0.35 0.95 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.225" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.035" length="0.45"/>
      </geometry>
    </collision>
  </link>

  <joint name="knee_pitch_R" type="revolute">
    <origin xyz="0 0 -0.45" rpy="0 0 0"/>
    <parent link="thigh_link_R"/>
    <child link="shank_link_R"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.5" upper="0.1" effort="250" velocity="10.0"/>
    <dynamics damping="0.3" friction="0.0"/>
  </joint>

  <link name="foot_link_R">
    <inertial>
      <origin xyz="0 0 -0.015" rpy="0 0 0"/>
      <mass value="1.0"/>
      <inertia ixx="0.0020" ixy="0.0" ixz="0.0"
               iyy="0.0040" iyz="0.0"
               izz="0.0020"/>
    </inertial>
    <visual>
      <origin xyz="0 0 -0.015" rpy="0 0 0"/>
      <geometry>
        <box size="0.24 0.08 0.03"/>
      </geometry>
      <material name="foot_gray_R">
        <color rgba="0.52 0.52 0.52 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 -0.015" rpy="0 0 0"/>
      <geometry>
        <box size="0.24 0.08 0.03"/>
      </geometry>
    </collision>
  </link>

  <joint name="ankle_pitch_R" type="revolute">
    <origin xyz="0 0 -0.45" rpy="0 0 0"/>
    <parent link="shank_link_R"/>
    <child link="foot_link_R"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.2" upper="0.8" effort="150" velocity="8.0"/>
    <dynamics damping="0.2" friction="0.0"/>
  </joint>

  <!-- Heel / Toe contact links (RIGHT) -->
  <link name="heel_contact_R">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1e-6"/>
      <inertia ixx="1e-9" ixy="0" ixz="0"
               iyy="1e-9" iyz="0"
               izz="1e-9"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry><sphere radius="0.01"/></geometry>
      <material name="heel_yellow_R"><color rgba="0.95 0.9 0.25 1.0"/></material>
    </visual>
  </link>

  <joint name="heel_contact_fixed_R" type="fixed">
    <parent link="foot_link_R"/>
    <child link="heel_contact_R"/>
    <origin xyz="-0.12 0 -0.03" rpy="0 0 0"/>
  </joint>

  <link name="toe_contact_R">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1e-6"/>
      <inertia ixx="1e-9" ixy="0" ixz="0"
               iyy="1e-9" iyz="0"
               izz="1e-9"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry><sphere radius="0.01"/></geometry>
      <material name="toe_orange_R"><color rgba="0.95 0.55 0.25 1.0"/></material>
    </visual>
  </link>

  <joint name="toe_contact_fixed_R" type="fixed">
    <parent link="foot_link_R"/>
    <child link="toe_contact_R"/>
    <origin xyz="0.12 0 -0.03" rpy="0 0 0"/>
  </joint>

</robot>
